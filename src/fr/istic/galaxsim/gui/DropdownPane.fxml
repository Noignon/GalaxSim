<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<fx:root maxHeight="-Infinity" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity" type="VBox" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <HBox style="-fx-background-color: #c5c8cd;" onMouseClicked="#toggleContent">
         <children>
            <Label fx:id="headerLabel" alignment="CENTER" maxWidth="Infinity" text="Label" HBox.hgrow="ALWAYS" />
            <StackPane maxHeight="18.0" maxWidth="18.0" minHeight="18.0" minWidth="18.0" prefHeight="18.0" prefWidth="18.0" HBox.hgrow="ALWAYS">
               <children>
                  <ImageView fx:id="showButton" fitHeight="18.0" fitWidth="18.0" layoutX="10.0" layoutY="10.0" pickOnBounds="true" preserveRatio="true" visible="false">
                     <image>
                        <Image url="@icons/drop_down.png" />
                     </image>
                  </ImageView>
                  <ImageView fx:id="hideButton" fitHeight="18.0" fitWidth="18.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@icons/drop_up.png" />
                     </image>
                  </ImageView>
               </children>
            </StackPane>
         </children></HBox>
   </children>
</fx:root>
